# Test: Token with 4 fields + method call with default in loop
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, value, line: int, column: int):
        self.ttype: int = ttype
        self.value = value
        self.line: int = line
        self.column: int = column

class Lexer:
    def __init__(self, source: str):
        self.source: str = source
        self.pos: int = 0
        self.line: int = 1
        self.column: int = 1
        self.tokens: List[Token] = []

    def at_end(self) -> bool:
        return self.pos >= len(self.source)

    def current_char(self) -> char:
        return self.source[self.pos]

    def advance(self):
        self.pos = self.pos + 1
        self.column = self.column + 1

    def make_token(self, ttype: int = 5) -> Token:
        return Token(ttype, "", self.line, self.column)

    def run(self):
        while not self.at_end():
            ch: char = self.current_char()

            if ch == 'a':
                t: Token = self.make_token()
                self.tokens.append(t)
                self.advance()
                continue

            self.advance()

def main() -> int:
    lexer: Lexer = Lexer("abc")
    lexer.run()
    n: int = len(lexer.tokens)
    if n == 1:
        print("OK\n")
    else:
        print("FAIL: ")
        c: char = chr(48 + n)
        print(c)
        print("\n")
    return 0
