# Test 38: Bitwise operators
def main():
    a: int32 = 5    # 0101
    b: int32 = 3    # 0011

    c: int32 = a | b  # 0111 = 7
    if c == 7:
        print("OR OK\n")

    d: int32 = a & b  # 0001 = 1
    if d == 1:
        print("AND OK\n")

    e: int32 = a ^ b  # 0110 = 6
    if e == 6:
        print("XOR OK\n")
