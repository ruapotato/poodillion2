# Test 48: Skip whitespace pattern (like lexer.skip_whitespace)
class Lexer:
    def __init__(self, text: str):
        self.text: str = text
        self.pos: int = 0
        self.length: int = len(text)

    def at_end(self) -> bool:
        return self.pos >= self.length

    def current_char(self) -> char:
        return self.text[self.pos]

    def advance(self):
        self.pos = self.pos + 1

    def skip_whitespace(self):
        while not self.at_end():
            ch: char = self.current_char()
            if ch == ' ' or ch == '\t' or ch == '\n':
                self.advance()
            else:
                break

def main() -> int:
    print("Skip whitespace test\n")

    lex: Lexer = Lexer("   hello")
    lex.skip_whitespace()

    if lex.pos == 3:
        print("Pos: OK\n")
    else:
        print("Pos: FAIL\n")

    ch: char = lex.current_char()
    if ch == 'h':
        print("Char: OK\n")
    else:
        print("Char: FAIL\n")

    print("Done\n")
    return 0
