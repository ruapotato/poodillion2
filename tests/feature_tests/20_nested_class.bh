# Test 20: Nested object references
class Inner:
    def __init__(self):
        self.val = 10

class Outer:
    def __init__(self):
        self.inner = Inner()

def main():
    o = Outer()
    if o.inner.val == 10:
        print("Nested class works\n")
    else:
        print("Nested class broken\n")
