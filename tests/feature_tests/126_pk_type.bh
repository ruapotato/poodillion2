# Minimal test: pk = peek_char() then compare with "'"
class Lexer:
    def __init__(self, source: str):
        self.source: str = source
        self.pos: int = 0

    def peek_char(self, offset: int = 1) -> char:
        pos: int = self.pos + offset
        if pos >= len(self.source):
            return ' '
        return self.source[pos]

    def run(self) -> int:
        pk = self.peek_char()
        if pk == '"' or pk == "'":
            return 1
        return 0

def main() -> int:
    # Source starts with f" so pos 0 is 'f', peek(1) returns '"'
    lexer: Lexer = Lexer("f\"hello\"")
    result: int = lexer.run()
    if result == 1:
        print("OK\n")
    else:
        print("FAIL\n")
    return 0
