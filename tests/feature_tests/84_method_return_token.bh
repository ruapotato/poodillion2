# Test 84: Token returned from method
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, line: int, column: int):
        self.ttype: int = ttype
        self.line: int = line
        self.column: int = column

class Lexer:
    def __init__(self):
        self.line: int = 1
        self.column: int = 1

    def make_token(self, ttype: int, start_line: int, start_col: int) -> Token:
        return Token(ttype, start_line, start_col)

def main() -> int:
    print("Method return token test\n")

    lexer: Lexer = Lexer()
    print("Lexer created\n")

    # Create token via method
    t: Token = lexer.make_token(42, 5, 10)
    print("Token created via method\n")

    if t.ttype == 42:
        print("t.ttype: OK\n")
    else:
        print("t.ttype: FAIL\n")

    if t.line == 5:
        print("t.line: OK\n")
    else:
        print("t.line: FAIL\n")

    if t.column == 10:
        print("t.column: OK\n")
    else:
        print("t.column: FAIL\n")

    print("Done\n")
    return 0
