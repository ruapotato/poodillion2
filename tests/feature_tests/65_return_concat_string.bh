# Test 65: Return concatenated string from method

class Builder:
    def build(self) -> str:
        result: str = ""
        result = result + "a"
        result = result + "b"
        result = result + "c"
        return result

def main() -> int:
    print("Return concat test\n")

    b: Builder = Builder()
    s: str = b.build()

    print("Got string\n")

    if s == "abc":
        print("Value: OK\n")
    else:
        print("Value: FAIL\n")

    print("Done\n")
    return 0
