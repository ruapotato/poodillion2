# Test 71: Self field access in method
try:
    from typing import List
except:
    pass

class Container:
    def __init__(self):
        self.items: List[int] = []

    def add(self, val: int):
        print("In add method\n")
        # Access self.items
        items_ref: List[int] = self.items
        print("Got items ref\n")
        items_ref.append(val)
        print("Appended value\n")

def main() -> int:
    print("Self field method test\n")

    c: Container = Container()
    print("Created container\n")

    c.add(10)
    print("After add\n")

    print("Done\n")
    return 0
