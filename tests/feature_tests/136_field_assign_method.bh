# Test field assignment from method call (like self.line = self.line + 1)
class Position:
    line: int
    col: int

    def init(self) -> Position:
        self.line = 1
        self.col = 1
        return self

    def advance(self) -> void:
        self.col = self.col + 1

    def newline(self) -> void:
        self.line = self.line + 1
        self.col = 1

def main() -> int:
    p: Position = Position().init()
    p.advance()
    p.advance()
    p.newline()
    p.advance()
    if p.line == 2 and p.col == 2:
        print("OK\n")
    else:
        print("FAIL\n")
    return 0
