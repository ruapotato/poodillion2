# Test method that returns a class instance
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, value: str, line: int, column: int):
        self.ttype: int = ttype
        self.value: str = value
        self.line: int = line
        self.column: int = column

class Lexer:
    def __init__(self, source: str):
        self.source: str = source
        self.pos: int = 0
        self.line: int = 1
        self.column: int = 1
        self.tokens: List[Token] = []

    def make_token(self, ttype: int, value: str) -> Token:
        start_line, start_col = self.line, self.column
        return Token(ttype, value, start_line, start_col)

    def add_token(self) -> int:
        tok: Token = self.make_token(59, "test")
        self.tokens.append(tok)
        return len(self.tokens)

def main() -> int:
    lexer: Lexer = Lexer("hello")
    n: int = lexer.add_token()
    if n == 1:
        print("OK\n")
    else:
        print("FAIL\n")
    return 0
