# Test 70: Class with List field - simple init and access
try:
    from typing import List
except:
    pass

class Container:
    def __init__(self):
        self.items: List[int] = []
        self.name: str = "test"

def main() -> int:
    print("Class list init test\n")

    c: Container = Container()
    print("Created container\n")

    # Access items directly
    c.items.append(10)
    print("Appended 10\n")

    n: int = len(c.items)
    if n == 1:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    print("Done\n")
    return 0
