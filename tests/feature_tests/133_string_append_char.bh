# Test appending char to string (like ident += ch)
def main() -> int:
    s: str = ""
    ch: char = 'x'
    s = s + ch
    ch = 'y'
    s = s + ch
    ch = 'z'
    s = s + ch
    if s == "xyz":
        print("OK\n")
    else:
        print("FAIL: ")
        print(s)
        print("\n")
    return 0
