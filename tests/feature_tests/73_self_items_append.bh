# Test 73: Append to self.items in method
try:
    from typing import List
except:
    pass

class Container:
    def __init__(self):
        self.items: List[int] = []

    def add_direct(self, val: int):
        print("Calling self.items.append\n")
        self.items.append(val)
        print("Done append\n")

def main() -> int:
    print("Self items append test\n")

    c: Container = Container()
    print("Created container\n")

    print("About to call c.add_direct\n")
    c.add_direct(42)
    print("After add_direct\n")

    n: int = len(c.items)
    if n == 1:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    print("Done\n")
    return 0
