# Minimal test: method call relying on default argument
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int):
        self.ttype: int = ttype

class Lexer:
    def __init__(self):
        self.tokens: List[Token] = []

    def make_token(self, ttype: int = 5) -> Token:
        return Token(ttype)

    def run(self):
        # Call WITHOUT providing the optional argument
        t: Token = self.make_token()
        self.tokens.append(t)

def main() -> int:
    lexer: Lexer = Lexer()
    lexer.run()
    n: int = len(lexer.tokens)
    if n == 1:
        print("OK\n")
    else:
        print("FAIL\n")
    return 0
