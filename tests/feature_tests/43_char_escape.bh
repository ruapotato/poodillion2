# Test 43: Char comparison with escape sequences
def main() -> int:
    print("Char escape test\n")

    # Test newline char
    nl: char = '\n'
    if nl == '\n':
        print("Newline: OK\n")
    else:
        print("Newline: FAIL\n")

    # Test tab char
    tb: char = '\t'
    if tb == '\t':
        print("Tab: OK\n")
    else:
        print("Tab: FAIL\n")

    # Test space
    sp: char = ' '
    if sp == ' ':
        print("Space: OK\n")
    else:
        print("Space: FAIL\n")

    # Test comparison in or expression
    ch: char = '\n'
    if ch == ' ' or ch == '\n':
        print("Or expr: OK\n")
    else:
        print("Or expr: FAIL\n")

    print("Done\n")
    return 0
