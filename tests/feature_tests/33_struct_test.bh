# Test 33: Simple struct/class creation
class MyClass:
    value: int32

    def __init__(self, v: int32):
        self.value = v

def main():
    print("Creating class...\n")
    obj: MyClass = MyClass(42)
    print("Class created\n")

    val: int32 = obj.value
    print("Value: ")
    if val == 42:
        print("42")
    else:
        print("not 42")
    print("\n")

    print("Done!\n")
