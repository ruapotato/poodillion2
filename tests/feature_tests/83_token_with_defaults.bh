# Test 83: Token with default arguments (like real lexer)
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, value: str, line: int, column: int, end_line: int = 0, end_column: int = 0):
        self.ttype: int = ttype
        self.value: str = value
        self.line: int = line
        self.column: int = column
        self.col: int = column
        self.end_line: int = end_line if end_line != 0 else line
        self.end_column: int = end_column if end_column != 0 else column

def main() -> int:
    print("Token with defaults test\n")

    # Create with all args
    t1: Token = Token(1, "a", 1, 1, 1, 2)
    print("t1 created\n")

    # Create with default args
    t2: Token = Token(2, "b", 5, 10)
    print("t2 created\n")

    # Check t1 fields
    if t1.ttype == 1:
        print("t1.ttype: OK\n")
    else:
        print("t1.ttype: FAIL\n")

    if t1.column == 1:
        print("t1.column: OK\n")
    else:
        print("t1.column: FAIL\n")

    if t1.end_line == 1:
        print("t1.end_line: OK\n")
    else:
        print("t1.end_line: FAIL\n")

    if t1.end_column == 2:
        print("t1.end_column: OK\n")
    else:
        print("t1.end_column: FAIL\n")

    # Check t2 fields (using defaults)
    if t2.ttype == 2:
        print("t2.ttype: OK\n")
    else:
        print("t2.ttype: FAIL\n")

    if t2.column == 10:
        print("t2.column: OK\n")
    else:
        print("t2.column: FAIL\n")

    # end_line should default to line since end_line arg was 0
    if t2.end_line == 5:
        print("t2.end_line: OK\n")
    else:
        print("t2.end_line: FAIL\n")

    # end_column should default to column since end_column arg was 0
    if t2.end_column == 10:
        print("t2.end_column: OK\n")
    else:
        print("t2.end_column: FAIL\n")

    print("Done\n")
    return 0
