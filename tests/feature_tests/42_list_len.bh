# Test 42: List[int] with len() - Python-like list syntax
try:
    from typing import List
except:
    pass

def main() -> int:
    print("List len test\n")

    # Create empty list
    items: List[int] = []

    # Check initial length
    n: int = len(items)
    if n == 0:
        print("Empty: OK\n")
    else:
        print("Empty: FAIL\n")

    # Append items
    items.append(10)
    n = len(items)
    if n == 1:
        print("Len 1: OK\n")
    else:
        print("Len 1: FAIL\n")

    items.append(20)
    n = len(items)
    if n == 2:
        print("Len 2: OK\n")
    else:
        print("Len 2: FAIL\n")

    items.append(30)
    n = len(items)
    if n == 3:
        print("Len 3: OK\n")
    else:
        print("Len 3: FAIL\n")

    print("Done\n")
    return 0
