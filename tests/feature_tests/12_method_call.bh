# Test 12: Method calls
class Counter:
    def __init__(self):
        self.count = 0

    def inc(self):
        self.count = self.count + 1

    def get(self) -> int:
        return self.count

def main():
    c = Counter()
    c.inc()
    c.inc()
    if c.get() == 2:
        print("Method calls work\n")
    else:
        print("Method calls broken\n")
