# Test 64: Accessing class objects from List
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, value: str):
        self.value: str = value

def main() -> int:
    print("List class access test\n")

    tokens: List[Token] = []

    t1: Token = Token("hello")
    tokens.append(t1)

    t2: Token = Token("world")
    tokens.append(t2)

    n: int = len(tokens)
    if n == 2:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    # Access first token
    print("Accessing t1...\n")
    first: Token = tokens[0]
    print("Got first\n")

    if first.value == "hello":
        print("First: OK\n")
    else:
        print("First: FAIL\n")

    print("Done\n")
    return 0
