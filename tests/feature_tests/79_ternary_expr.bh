# Test 79: Ternary conditional expression
def main() -> int:
    print("Ternary test\n")

    x: int = 5
    y: int = 10 if x > 3 else 20
    if y == 10:
        print("y: OK\n")
    else:
        print("y: FAIL\n")

    z: int = 30 if x < 3 else 40
    if z == 40:
        print("z: OK\n")
    else:
        print("z: FAIL\n")

    # Test 0 condition
    a: int = 0
    b: int = 100 if a != 0 else 200
    if b == 200:
        print("b: OK\n")
    else:
        print("b: FAIL\n")

    print("Done\n")
    return 0
