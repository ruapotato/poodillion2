# Test 22: Class with list field (like Lexer.tokens)
class TokenList:
    def __init__(self):
        self.items = []
        self.count = 0

    def add(self, val: int):
        self.items.append(val)
        self.count = self.count + 1

def main():
    tl = TokenList()
    tl.add(10)
    tl.add(20)
    if tl.count == 2:
        print("Class list works\n")
    else:
        print("Class list broken\n")
