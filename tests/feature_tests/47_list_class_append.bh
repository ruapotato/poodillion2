# Test 47: List of class objects with append (like List[Token])
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, kind: int, val: str):
        self.kind: int = kind
        self.val: str = val

def main() -> int:
    print("List class append test\n")

    tokens: List[Token] = []

    # Append tokens
    t1: Token = Token(1, "hello")
    tokens.append(t1)

    t2: Token = Token(2, "world")
    tokens.append(t2)

    n: int = len(tokens)
    if n == 2:
        print("Len: OK\n")
    else:
        print("Len: FAIL\n")

    print("Done\n")
    return 0
