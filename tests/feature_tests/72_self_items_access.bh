# Test 72: Just access self.items in method
try:
    from typing import List
except:
    pass

class Container:
    def __init__(self):
        self.items: List[int] = []

    def get_items_len(self) -> int:
        print("Getting len of self.items\n")
        n: int = len(self.items)
        print("Got len\n")
        return n

def main() -> int:
    print("Self items access test\n")

    c: Container = Container()
    print("Created container\n")

    n: int = c.get_items_len()
    print("Got result\n")

    if n == 0:
        print("Len: OK\n")
    else:
        print("Len: FAIL\n")

    print("Done\n")
    return 0
