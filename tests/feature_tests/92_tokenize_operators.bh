# Test tokenize with operator elif chain
try:
    from typing import List
except:
    pass

TT_PLUS: int = 67
TT_MINUS: int = 68
TT_STAR: int = 69
TT_EQUALS: int = 74
TT_COLON: int = 117

class Token:
    def __init__(self, ttype: int, value, line: int, column: int):
        self.ttype: int = ttype
        self.value = value
        self.line: int = line
        self.column: int = column

class Lexer:
    def __init__(self, source: str):
        self.source: str = source
        self.pos: int = 0
        self.line: int = 1
        self.column: int = 1
        self.tokens: List[Token] = []

    def at_end(self) -> bool:
        return self.pos >= len(self.source)

    def current_char(self) -> char:
        return self.source[self.pos]

    def advance(self):
        self.pos = self.pos + 1
        self.column = self.column + 1

    def tokenize(self) -> List[Token]:
        while not self.at_end():
            ch: char = self.current_char()
            start_line: int = self.line
            start_col: int = self.column

            if ch == ' ':
                self.advance()
                continue

            if ch == '+':
                self.advance()
                self.tokens.append(Token(TT_PLUS, None, start_line, start_col))
            elif ch == '-':
                self.advance()
                self.tokens.append(Token(TT_MINUS, None, start_line, start_col))
            elif ch == '*':
                self.advance()
                self.tokens.append(Token(TT_STAR, None, start_line, start_col))
            elif ch == '=':
                self.advance()
                self.tokens.append(Token(TT_EQUALS, None, start_line, start_col))
            elif ch == ':':
                self.advance()
                self.tokens.append(Token(TT_COLON, None, start_line, start_col))
            else:
                self.advance()

        return self.tokens

def main() -> int:
    code: str = "+ - * = :"
    lexer: Lexer = Lexer(code)
    tokens: List[Token] = lexer.tokenize()
    n: int = len(tokens)
    if n == 5:
        t0: Token = tokens[0]
        t4: Token = tokens[4]
        if t0.ttype == TT_PLUS and t4.ttype == TT_COLON:
            print("OK\n")
        else:
            print("FAIL: wrong types\n")
    else:
        print("FAIL: count=")
        c: char = chr(48 + n)
        print(c)
        print("\n")
    return 0
