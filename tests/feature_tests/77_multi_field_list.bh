# Test 77: Multi-field Token in List
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, value: str, line: int, column: int):
        self.ttype: int = ttype
        self.value: str = value
        self.line: int = line
        self.column: int = column

def main() -> int:
    print("Multi-field Token in List test\n")

    tokens: List[Token] = []

    # Create tokens and add to list
    t1: Token = Token(1, "hello", 1, 1)
    tokens.append(t1)
    print("Added t1\n")

    t2: Token = Token(2, "world", 1, 7)
    tokens.append(t2)
    print("Added t2\n")

    # Check count
    n: int = len(tokens)
    if n == 2:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    # Retrieve and check fields
    first: Token = tokens[0]
    print("Got first\n")

    if first.ttype == 1:
        print("first.ttype: OK\n")
    else:
        print("first.ttype: FAIL\n")

    if first.value == "hello":
        print("first.value: OK\n")
    else:
        print("first.value: FAIL\n")

    if first.line == 1:
        print("first.line: OK\n")
    else:
        print("first.line: FAIL\n")

    if first.column == 1:
        print("first.column: OK\n")
    else:
        print("first.column: FAIL\n")

    # Check second token
    second: Token = tokens[1]
    if second.value == "world":
        print("second.value: OK\n")
    else:
        print("second.value: FAIL\n")

    print("Done\n")
    return 0
