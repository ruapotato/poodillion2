# Test 44: While loop with char comparison
def main() -> int:
    print("Char while test\n")

    text: str = "hello world"
    i: int = 0
    n: int = len(text)

    # Count spaces using while loop
    count: int = 0
    while i < n:
        ch: char = text[i]
        if ch == ' ':
            count = count + 1
        i = i + 1

    if count == 1:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    print("Done\n")
    return 0
