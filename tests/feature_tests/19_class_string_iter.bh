# Test 19: Class that iterates over string (Lexer-like)
class StringWalker:
    def __init__(self, text: str):
        self.text = text
        self.pos = 0

    def current(self) -> str:
        if self.pos >= len(self.text):
            return ""
        return self.text[self.pos]

    def advance(self):
        self.pos = self.pos + 1

def main():
    w = StringWalker("hello")
    count = 0
    while w.pos < len(w.text):
        w.advance()
        count = count + 1
    if count == 5:
        print("Class string iter works\n")
    else:
        print("Class string iter broken\n")
