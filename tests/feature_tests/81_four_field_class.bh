# Test 81: Class with 4 fields (no aliases)
try:
    from typing import List
except:
    pass

class Token:
    def __init__(self, ttype: int, value: str, line: int, column: int):
        self.ttype: int = ttype
        self.value: str = value
        self.line: int = line
        self.column: int = column

def main() -> int:
    print("Four field class test\n")

    t: Token = Token(42, "hello", 10, 5)
    print("Token created\n")

    if t.ttype == 42:
        print("ttype: OK\n")
    else:
        print("ttype: FAIL\n")

    if t.value == "hello":
        print("value: OK\n")
    else:
        print("value: FAIL\n")

    if t.line == 10:
        print("line: OK\n")
    else:
        print("line: FAIL\n")

    if t.column == 5:
        print("column: OK\n")
    else:
        print("column: FAIL\n")

    print("Done\n")
    return 0
