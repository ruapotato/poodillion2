# Test 58: Default parameter values
class Token:
    def __init__(self, kind: int, line: int, end_line: int = 0):
        self.kind: int = kind
        self.line: int = line
        # Use default if end_line is 0
        if end_line != 0:
            self.end_line: int = end_line
        else:
            self.end_line: int = line

def main() -> int:
    print("Default params test\n")

    # Without optional param
    t1: Token = Token(1, 10)
    if t1.end_line == 10:
        print("Default: OK\n")
    else:
        print("Default: FAIL\n")

    # With optional param
    t2: Token = Token(2, 10, 15)
    if t2.end_line == 15:
        print("Explicit: OK\n")
    else:
        print("Explicit: FAIL\n")

    print("Done\n")
    return 0
