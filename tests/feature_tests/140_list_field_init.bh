# Test class with List field initialized in __init__
class Tracker:
    stack: List[int]
    flag: int

    def __init__(self, val: int):
        self.stack = [val]
        self.flag = 1

    def push(self, v: int) -> void:
        self.stack.append(v)

    def size(self) -> int:
        return len(self.stack)

def main() -> int:
    print("Creating\n")
    t: Tracker = Tracker(0)
    print("Created\n")
    t.push(1)
    t.push(2)
    n: int = t.size()
    if n == 3:
        print("OK\n")
    else:
        print("FAIL\n")
    return 0
