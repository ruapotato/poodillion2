# Test 62: String concatenation in a loop (simulates lexer's read_identifier)
def main() -> int:
    print("String loop concat test\n")

    code: str = "abc123"
    pos: int = 0
    ident: str = ""

    # Simulate lexer's read_identifier loop
    # Note: code[pos] returns a char (byte), not a string
    while pos < len(code):
        ch: char = code[pos]
        ident = ident + ch
        pos = pos + 1

    if ident == "abc123":
        print("Loop concat: OK\n")
    else:
        print("Loop concat: FAIL\n")

    print("Done\n")
    return 0
