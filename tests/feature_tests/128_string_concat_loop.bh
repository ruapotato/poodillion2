# Test string concatenation in a loop (like ident += ch in read_identifier)
def main() -> int:
    result: str = ""
    i: int = 0
    while i < 3:
        if i == 0:
            result = result + "a"
        elif i == 1:
            result = result + "b"
        else:
            result = result + "c"
        i = i + 1
    if result == "abc":
        print("OK\n")
    else:
        print("FAIL: ")
        print(result)
        print("\n")
    return 0
