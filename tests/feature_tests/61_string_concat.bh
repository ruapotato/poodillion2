# Test 61: String concatenation (ident += ch)
def main() -> int:
    print("String concat test\n")

    s: str = ""
    s = s + "a"
    s = s + "b"
    s = s + "c"

    if s == "abc":
        print("Concat: OK\n")
    else:
        print("Concat: FAIL\n")

    # Also test += operator
    s2: str = ""
    s2 += "x"
    s2 += "y"

    if s2 == "xy":
        print("PlusEq: OK\n")
    else:
        print("PlusEq: FAIL\n")

    print("Done\n")
    return 0
