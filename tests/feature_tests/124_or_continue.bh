# Minimal: compound or + default method call + continue in loop
class Foo:
    def __init__(self):
        self.val: int = 0

    def make(self, x: int = 5) -> int:
        return x

    def run(self, s: str):
        i: int = 0
        while i < len(s):
            ch: char = s[i]
            if ch == 'a' or ch == 'b':
                result: int = self.make()
                self.val = self.val + result
                i = i + 1
                continue
            i = i + 1

def main() -> int:
    f: Foo = Foo()
    f.run("abc")
    if f.val == 10:
        print("OK\n")
    else:
        print("FAIL: ")
        c: char = chr(48 + f.val)
        print(c)
        print("\n")
    return 0
