# Test 31: Command line arguments
from lib.sys import argc, argv

def main():
    print("Testing argc/argv\n")
    n: int32 = argc()

    # Print argc using sign check
    print("argc sign: ")
    if n < 0:
        print("negative\n")
    elif n > 100:
        print("very large\n")
    else:
        print("normal\n")

    print("argc = ")
    if n == 3:
        print("3")
    else:
        print("not 3")
    print("\n")

    print("Testing argv:\n")
    arg0: *uint8 = argv(0)
    print(arg0)
    print("\n")
