# Test 68: Class with two fields
try:
    from typing import List
except:
    pass

class Item:
    def __init__(self, id: int, name: str):
        self.id: int = id
        self.name: str = name

def main() -> int:
    print("Two field class test\n")

    items: List[Item] = []

    i: Item = Item(1, "hello")
    items.append(i)
    print("Added item\n")

    n: int = len(items)
    if n == 1:
        print("Count: OK\n")
    else:
        print("Count: FAIL\n")

    # Retrieve
    first: Item = items[0]
    print("Got first\n")

    # Access first field
    fid: int = first.id
    print("Got id\n")
    if fid == 1:
        print("ID: OK\n")
    else:
        print("ID: FAIL\n")

    # Access second field
    fname: str = first.name
    print("Got name\n")
    print(fname)
    print("\n")
    if fname == "hello":
        print("Name: OK\n")
    else:
        print("Name: FAIL\n")

    print("Done\n")
    return 0
