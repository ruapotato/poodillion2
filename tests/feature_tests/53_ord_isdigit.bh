# Test 53: ord() and is_digit pattern
def is_digit(c: int) -> int:
    if c >= 48 and c <= 57:  # '0' to '9'
        return 1
    return 0

def is_alpha(c: int) -> int:
    if c >= 65 and c <= 90:  # 'A' to 'Z'
        return 1
    if c >= 97 and c <= 122:  # 'a' to 'z'
        return 1
    return 0

def main() -> int:
    print("Ord isdigit test\n")

    ch: char = '5'
    code: int = ord(ch)
    if is_digit(code) == 1:
        print("5 is digit: OK\n")
    else:
        print("5 is digit: FAIL\n")

    ch = 'a'
    code = ord(ch)
    if is_digit(code) == 0:
        print("a not digit: OK\n")
    else:
        print("a not digit: FAIL\n")

    if is_alpha(code) == 1:
        print("a is alpha: OK\n")
    else:
        print("a is alpha: FAIL\n")

    ch = 'Z'
    code = ord(ch)
    if is_alpha(code) == 1:
        print("Z is alpha: OK\n")
    else:
        print("Z is alpha: FAIL\n")

    print("Done\n")
    return 0
