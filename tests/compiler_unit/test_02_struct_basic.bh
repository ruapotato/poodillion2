from lib.syscalls import *

class Counter:
    value: int32

def Counter_inc(self: Ptr[Counter]):
    self.value = self.value + 1

def main() -> int32:
    c: Counter
    c.value = 10
    Counter_inc(addr(c))
    Counter_inc(addr(c))
    print_int(c.value)
    println("")
    return 0
