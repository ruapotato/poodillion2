def vtn_in_parse_int(buf: Ptr[uint8], pos: Ptr[int32]) -> int32:
    result: int32 = 0
    i: int32 = pos[0]
    while (buf[i] == cast[uint8](59)):
        i = (i + 1)
    while ((buf[i] >= cast[uint8](48)) and (buf[i] <= cast[uint8](57))):
        result = ((result * 10) + (cast[int32](buf[i]) - 48))
        i = (i + 1)
    if (buf[i] == cast[uint8](59)):
        i = (i + 1)
    pos[0] = i
    return result

def vtn_process_input_event():
    pos: int32 = 0
    if (vtn_in_evt_buf[0] == cast[uint8](107)):
        if (vtn_in_evt_buf[1] == cast[uint8](100)):
            vtn_evt_type = VTN_EVT_KEY_DOWN
        elif (vtn_in_evt_buf[1] == cast[uint8](117)):
            vtn_evt_type = VTN_EVT_KEY_UP
        elif (vtn_in_evt_buf[1] == cast[uint8](114)):
            vtn_evt_type = VTN_EVT_KEY_REPEAT
        elif (vtn_in_evt_buf[1] == cast[uint8](99)):
            vtn_evt_type = VTN_EVT_KEY_CHAR
        vtn_evt_keycode = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        vtn_evt_scancode = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        vtn_evt_modifiers = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        vtn_evt_timestamp = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
    elif (vtn_in_evt_buf[0] == cast[uint8](109)):
        if (vtn_in_evt_buf[1] == cast[uint8](100)):
            vtn_evt_type = VTN_EVT_MOUSE_DOWN
            vtn_evt_button = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_x = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_y = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_modifiers = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_timestamp = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        elif (vtn_in_evt_buf[1] == cast[uint8](117)):
            vtn_evt_type = VTN_EVT_MOUSE_UP
            vtn_evt_button = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_x = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_y = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_modifiers = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_timestamp = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        elif (vtn_in_evt_buf[1] == cast[uint8](109)):
            vtn_evt_type = VTN_EVT_MOUSE_MOVE
            vtn_evt_x = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_y = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_dx = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_dy = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_modifiers = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_timestamp = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
        elif (vtn_in_evt_buf[1] == cast[uint8](119)):
            vtn_evt_type = VTN_EVT_MOUSE_WHEEL
            vtn_evt_dx = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_dy = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
            vtn_evt_x = vtn_in_parse_int(Ptr[uint8](addr(vtn_in_param_buf)), Ptr[int32](addr(pos)))
