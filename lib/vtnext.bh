from lib.syscalls import *

VTN_BUF_SIZE: Final[int32] = 8192

vtn_buffer: Array[8192, uint8]

vtn_buf_pos: int32 = 0

def vtn_flush():
    if (vtn_buf_pos > 0):

        write(STDOUT, Ptr[uint8](addr(vtn_buffer)), vtn_buf_pos)
        vtn_buf_pos = 0

def vtn_write_byte(b: int32):
    if (vtn_buf_pos >= (VTN_BUF_SIZE - 1)):
        vtn_flush()
    vtn_buffer[vtn_buf_pos] = cast[uint8](b)
    vtn_buf_pos = (vtn_buf_pos + 1)

def vtn_write_str(s: Ptr[uint8]):
    i: int32 = 0
    while (s[i] != cast[uint8](0)):
        vtn_write_byte(cast[int32](s[i]))
        i = (i + 1)

def vtn_write_int(n: int32):
    num: int32 = n
    if (num < 0):
        vtn_write_byte(45)
        num = (0 - num)
    if (num == 0):
        vtn_write_byte(48)
        return
    temp: int32 = 0
    count: int32 = 0
    while (num > 0):
        temp = ((temp * 10) + (num % 10))
        num = (num / 10)
        count = (count + 1)
    while ((temp > 0) or (count > 0)):
        vtn_write_byte((48 + (temp % 10)))
        temp = (temp / 10)
        count = (count - 1)

def vtn_write_float(f: int32):
    vtn_write_int(f)
    vtn_write_byte(46)
    vtn_write_byte(48)

def vtn_begin_cmd(cmd: Ptr[uint8]):
    vtn_write_byte(27)
    vtn_write_byte(93)
    vtn_write_str(Ptr[uint8]("vtn;"))
    vtn_write_str(cmd)
    vtn_write_byte(59)

def vtn_end_cmd():
    vtn_write_byte(7)

def vtn_sep():
    vtn_write_byte(59)

